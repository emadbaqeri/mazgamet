{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## 📚 Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["test"]
    },
    {
      "title": "## 🧰 Maintenance",
      "labels": ["chore", "dependencies"]
    },
    {
      "title": "## 💥 Breaking Changes",
      "labels": ["breaking"]
    }
  ],
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "No changes",
  "label_extractor": [
    {
      "pattern": "^feat:\\s.+",
      "target": "feature"
    },
    {
      "pattern": "^fix:\\s.+",
      "target": "fix"
    },
    {
      "pattern": "^docs:\\s.+",
      "target": "documentation"
    },
    {
      "pattern": "^test:\\s.+",
      "target": "test"
    },
    {
      "pattern": "^chore:\\s.+",
      "target": "chore"
    },
    {
      "pattern": "^break:\\s.+",
      "target": "breaking"
    }
  ],
  "transformers": [
    {
      "pattern": "^([A-Z]+)\\-\\d+\\s.+$",
      "target": "[$1-$2] $3"
    }
  ],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "ignore_pre_releases": true,
  "filter_regex_title": "(?:^.[^)]*)\\):",
  "filter_regex_exclude": "",
  "reference": {
    "current": "current/head",
    "target": "target/base"
  },
  "base_branches": ["main", "master"],
  "max_commits_count": 100,
  "tag_resolver": {
    "method": "semver",
    "filter": "^v"
  },
  "duplicate_filter": {
    "pattern": "(?:.*)?\\s\\((?:#\\d+(?:, )?)+\\)",
    "on_property": "title",
    "method": "match"
  }
}